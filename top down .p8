function enter_room(room)
  room_count[room] += 1
  if room == 1 and room_count[1] == 1 then
    music(0, 600)
    
  end
  if room == 5 
  and room_count[5] == 1 then
    curr_scene.text = {
    [[ hola sprout boy ]],
     
