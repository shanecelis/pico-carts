pages = { 

[[
winnie the pooh: 
search for honey

by shane celis
(c) 2021/10/11
]],

[[
pooh awoke to a rumbly in his
tummy. "oh dear, i must find
something to eat."
]],

[[
"say piglet, you wouldn't happen
to have any honey, would you?"

"oh, d-d-dear me, no, pooh,"
piglet said.
]]

}











current_page = 1

function _update()
  if(btnp(1)) then
    current_page += 1
  end
  if(btnp(0)) then
    current_page -= 1
  end
  current_page = clamp(current_page, 1, #pages)
end

function draw_page(page)
  local i = page - 1
		map(i * 16,0, 0,0, 16, 8)
		print(pages[page], 0, 64)
end

function _draw()
  cls()
  draw_page(current_page)
end

function clamp(x, a, b)
  return max(a, min(b,x))
end
