function std_anim(start, walk_speed)
  return {
    stand= {
      ticks=1,--how long is each frame shown.
      frames={start+1},--what frames are shown.
    },
    walk= {
      ticks=walk_speed or 5,
      frames={start+2,start+3,start+4,start+5},
    },
    jump= {
      ticks=1,
      frames={start},
    },
    slide= {
      ticks=1,
      frames={start+6},
    },
  }
end
----------------
-- 5 hits=die
-- 3 -sprite 9
p1=jumper:new { 
x=44, 
y=30,
health=4,
anims = std_anim(1),
}

trickyshadow=jumper:new { 
x=39*8, 
y=14*8,
health=4,
anims=std_anim(49),
left=false,
right=false,
timetochange=0,
update=function(self)
  if time() > self.timetochange then
    self.left = rnd(2) > 1
    self.jump = rnd(2) > 1
    self.right = rnd(2) > 1
    self.timetochange=time() + 1
  end
  self:move(self.left, 
            self.right, 
            self.jump)
--  self:move(btn(1), btn(0))
end,
}
intro = scene:new {
  update = function(self)
    p1:update()

    if p1.y > 128 then
      p1.x = 18 * 8
      p1.y = 0
      -- return game
      return falling
    end
  end,

  draw = function(self)
    cls(12)

    p1:draw()
    trickyshadow:draw()
    map(0,0,0,0,128,128)

    --hud
    -- camera(0,0)
    print("not so well (shane)",24,4,7,0,0)
  end
}

function strokes()
  local front = emitter:new(
    {
      particle_class = particle:new {
        draw = function(self)
          line(self.pos.x, self.pos.y, self.pos.x, self.pos.y + self.size, self.colour)
        end
      }
    }, 64, 128, 0.2, 0)
  front.area = vec(128, 0)
  front.p_colours:set({7})
  front.p_size:set(5, 5)
  front.p_speed:set(64, 10)
  front.p_speed_final:set(84)
  front.p_life:set(1.5, 1)
  front.p_angle:set(90)
  return front
end

falling = scene:new {

  draw = function(self)

    scene.draw(self)
    p1:draw()
    -- map(0,0,0,0,128,128)

    --hud
    -- camera(0,0)
    -- print("not so well (shane)",24,4,7,0,0)
  end
}
add(falling,
    strokes())


game = scene:new {
  cam=dolly:new(
    {
      pos_min = vec(128 + 64, 64)

    }, p1),
  update = function(self)
    p1:update()
    trickyshadow:update()
    slime:update()
    self.cam:update()
    if p1.health <= 0 then 
      return death
    end
  end,

  draw = function(self)
    cls(0)

    camera(self.cam:cam_pos())

    p1:draw()
    slime:draw()
    trickyshadow:draw()
    map(0,0,0,0,128,128)

    --hud
    camera(0,0)
    print("not so well (shane)",24,4,7,0,0)
    down=111
--    spr(8.,18,down)
--    spr(8.,28,down)
--    spr(8.,38,down)
--    spr(8.,48,down)
    
    for loopy=1,4 do
      fullheart=8
      emptyheart=10
      if p1.health >= loopy then  
        spr(fullheart,8 + loopy * 10,down)
      else
        spr(emptyheart,8 + loopy * 10,down)
      end
    end
   
--    for i=1, do
--      spr(8, 8 + i * 10, down - 10)
--    end
  end,
}

death = scene:new {
  draw = function(self)
    cls(0)
    camera(0,0)
    spr(123, 64,42)
    spr(93, 64, 50)
    print("you died. continue or not?", 
      20,
      64)
  end
}

slime = actor:new({
  update = function(self)
    if self:in_bounds(p1.x, p1.y) then
      half=1/60
      p1.health = p1.health-half
      sfx(4)
    end
  end
}, 59,
18 * 8, 14 * 8)
