pico-8 cartridge // http://www.pico-8.com
version 41
__lua__

#include lib/vector.p8
#include lib/scene.p8
#include lib/actor.p8
#include lib/platformer.p8
#include lib/dolly.p8


p1=jumper:new { x=44, y=30 }
intro = scene:new {
  update = function(self)
    p1:update()

    if p1.y > 128 then
      p1.x = 18 * 8
      p1.y = 0
      return game
    end
  end,

  draw = function(self)

    cls(12)


    p1:draw()
    map(0,0,0,0,128,128)

    --hud
    -- camera(0,0)
    print("not so well (shane)",24,4,7,0,0)
  end
}


game = scene:new {
  cam=dolly:new(
    {
      pos_min = vec(128 + 64, 64)

    }, p1),
  update = function(self)
    p1:update()
    self.cam:update()
  end,

  draw = function(self)

    cls(0)

    camera(self.cam:cam_pos())

    p1:draw()
    map(0,0,0,0,128,128)

    --hud
    -- camera(0,0)
    print("not so well (shane)",24,4,7,0,0)
  end,
}

intro:install()

