end 

function _update()
if btn(⬇️) then
roboty=1+roboty
end 
if btn(⬆️) then
roboty=-1+roboty
end 

if btn(➡️) then
roboty=1+roboty
end 
if btn(⬆️) then
roboty=-1+roboty
end 
end
