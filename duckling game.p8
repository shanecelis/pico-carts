pico-8 cartridge // http://www.pico-8.com
version 41
__lua__

#include lib/vector.p8
#include lib/actor.p8
#include lib/platformer.p8
#include lib/dolly.p8


--_init the game to its initial
--state.
function _init()
  p1=jumper:new { x=44, y=100 }
  p1:set_anim("walk")
  cam=dolly:new(nil, p1)
  music(00)
end

--p8 functions
--------------------------------

function _update60()
  p1:update()
  cam:update()
  --demo camera shake
--  if(btnp(4))cam:shake(15,2)
  
  if p1.y >21*8 then
    p1.x = 44
    p1.y = 100
    for i=1,#coins do
      coins[i].collected = false
    end
  end
end

coins = {
{x=14*8, y=12*8},
{x=29*8, y=12*8},
{x=76*8, y=09*8},
{x=87*8, y=13*8},
{x=103*8, y=12*8},
}

function _draw()

  cls(0)
  camera(0,0)
--  print("dist "..dist(p1, coin), 0, 0)
  

  camera(cam:cam_pos())

  map(0,0,0,0,128,128)

  for i = 1,#coins do
    if coins[i].collected then
      -- collected coins don't do anything
    elseif dist(p1, coins[i]) <= 4 then
      -- collect coin
      coins[i].collected = true
    else
      spr(134,coins[i].x,coins[i].y)
    end
  end
 
  p1:draw()
  
end

function dist(a,b)
  return abs(a.x - b.x) + abs(a.y - b.y)
end

