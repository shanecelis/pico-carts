pico-8 cartridge // http://www.pico-8.com
version 33
__lua__
-- fisht
page = 0
x = 0
y = 0
function _init()

end

-- look for buttons
function _update()

if btn(➡️) then
  x = x + 1
end

if btn(⬅️) then
  x = x - 1
end

if btn(⬆️) then 
  y = y - 1
end

if btn(⬇️) then
  y = y +1
end


end

function _draw()
if page == 0 then
cls(12)
map(0, 0, 0, 0, 16, 16)
else
--make it blue
--clear colr 12
cls(12)
spr(1, x, y)
end



print("x = " ..	 x)
print("y = " .. y)
print("page = " ..	 page)

--if esc tern to page
--if x tern to page
if btnp(❎) then 
--tern to page 
--1+2 
page=12
--page change
end

end
