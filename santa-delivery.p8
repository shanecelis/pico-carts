#include text-box.p8
#include plist.p8
#include page.p8
#include message.lua
#include util.p8
-- book code

--_book = book:new({ page_class = cardinal_page }, pages)
_book = book:new({}, pages)
for i, p in pairs(_book.pages) do
  if i > 100 then
    local default_page = flr(i/100)
    if (not p.scene or p.scene == i) p.scene = default_page
    if (not p.nextpage) p.nextpage = default_page
  end
end
records = nil

local p5 = _book.pages[5]

p5.active = function(self, yes)
  page.active(self, yes)
  self.startat = time()
  self.carrots = 11
  self.stop_time = nil
  self.m = nil
  self[1] = nil
end

p5.time_left = function(self)
  if (self.stop_time) return self.stop_time
  local t = time() - self.startat
  return max(flr(30 - t),0)
end

p5.update = function(self)
  if self.carrots > 0 and self:time_left() > 0 then
    local pressed = false
    for i = 0, 5 do

      if btnp(i) then
        pressed = true
      end
    end
    if (pressed) self.carrots -= 1
    if self.carrots <= 0 then
      -- we won
      self.stop_time = self:time_left()
      add(self, "you did it!")
      self.nextpage = 6
    end
    if self:time_left() <= 0 then
      -- we lost
      add(self, "oh bother")
      self.nextpage = 4
    end
  else
    page.update(self)
  end
end

p5.draw = function(self)
  page.draw(self)

  print(self:time_left() .. "S", 60, 20)
  print(self.carrots .. " carrots left", 60, 27)
end

-- _book:set_page(p5)

function _init()
--  scan_sprites()
  l = plist:new(nil, {1, 2, 3, 4})
  printh(dump(l))
  printh(dump(l.keys))
  printh("count " ..#l)
  l['a'] = 5
  printh(dump(l))
  -- stop()
end

function _update()
  _book._page:update()
end


function _draw()
  _book._page:draw()
  -- if records ~= nil and frame % 20 == 0 then
	-- 	  anim_map(records)
	-- end
end

